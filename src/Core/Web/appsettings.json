{
  "Serilog": {
    "Using": [ "Serilog.Exceptions", "Serilog", "Serilog.Sinks.Console", "Serilog.Exceptions.EntityFrameworkCore" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ],
    "Properties": {
      "Application": "LayeredTemplate"
    }
  },
  "AllowedHosts": "*",

  "USE_MOCK_AUTH": true,
  "MOCK_EMAIL_SENDER": true,

  "AppSettings": {
  },

  "JsonPolymorphismSettings": {
    "AssemblyNames": [ "Application" ],
    "Mapping": {
      "LayeredTemplate.Application.Features.Figures.Models.Figure, Application": {
        "LayeredTemplate.Application.Features.Figures.Models.Circle, Application": "Circle",
        "LayeredTemplate.Application.Features.Figures.Models.Rectangle, Application": "Rectangle",
        "LayeredTemplate.Application.Features.Figures.Models.Square, Application": "Square"
      }
    }
  }
}